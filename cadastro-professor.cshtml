@{
    Layout = "_Master.cshtml";
    Page.Title = "Cadastro de professores";
}
<h1>Cadastro de professores</h1>
<form method="post" action="feed">
    <div class="formulario"><label>Nome do professor: </label><input class="c1" type="text" required><br></div>
    <div class="formulario"><label>Curso: </label><select class="select" required><option></option><option>Administração</option><option>Controle Ambiental</option><option>Edificações</option><option>Eletrotécnica</option><option>Geologia</option><option>Informática para Internet</option><option>Manutenção e Suporte em Informática</option><option>Mecânica</option><option>Mineração</option></select><br></div>
    <div class="formulario"><label>Matéria: </label><input class="c2" type="text" required><br></div>
    <input type="submit" id="enviar">
</form>